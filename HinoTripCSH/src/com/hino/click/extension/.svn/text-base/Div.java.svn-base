package com.hino.click.extension;

import org.apache.click.control.AbstractContainer;

public class Div extends AbstractContainer {
	private static final long serialVersionUID = -7567189299182825901L;

	public Div(String name) {
		super(name);
	}

	@Override
	public String getTag() {
		return "div";
	}
	
	public void setCss(String cssName)
	{
		setAttribute("class", cssName);
	}
	
}
