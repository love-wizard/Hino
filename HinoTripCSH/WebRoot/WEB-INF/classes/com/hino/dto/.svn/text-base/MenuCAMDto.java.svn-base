package com.hino.dto;

import org.apache.commons.fileupload.FileItem;

public class MenuCAMDto {

	private String id;
	private String name;
	private String description;
	
	private String name_en;
	private String description_en;
	public String getName_en() {
		return name_en;
	}

	public void setName_en(String nameEn) {
		name_en = nameEn;
	}

	public String getDescription_en() {
		return description_en;
	}

	public void setDescription_en(String descriptionEn) {
		description_en = descriptionEn;
	}

	private FileItem image;
	private FileItem image_en;

	public FileItem getImage_en() {
		return image_en;
	}

	public void setImage_en(FileItem imageEn) {
		image_en = imageEn;
	}

	public FileItem getImage() {
		return image;
	}

	public void setImage(FileItem image) {
		this.image = image;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

}
