package com.hino.dto;

public class SalesRewardDto {
	private String staffno = "";
	private String fullname = "";
	private double sales_total = 0;
	private double sales_reward = 0;
	private int rep_order = 0;
	
	public String getStaffno() {
		return staffno;
	}
	public void setStaffno(String staffno) {
		this.staffno = staffno;
	}
	public String getFullname() {
		return fullname;
	}
	public void setFullname(String fullname) {
		this.fullname = fullname;
	}
	public double getSales_total() {
		return sales_total;
	}
	public void setSales_total(double salesTotal) {
		sales_total = salesTotal;
	}
	public double getSales_reward() {
		return sales_reward;
	}
	public void setSales_reward(double salesReward) {
		sales_reward = salesReward;
	}
	public int getRep_order() {
		return rep_order;
	}
	public void setRep_order(int repOrder) {
		rep_order = repOrder;
	}
	
}
