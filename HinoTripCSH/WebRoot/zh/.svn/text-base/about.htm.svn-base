    <script>
 $(document).ready(function() {
 
 	$("div.cate .title").click(function() {
 		var sId = $(this).attr("id");
 		var oUl = $(this).parent().find("ul");
 		var sTabsId;
 		var sConClzId;
 		
 		$(".about_content").each(function(index){
			$(this).css({"display": "none"});
		});
 		
 		if(undefined == $(oUl).html() || null == $(oUl).html()) {
 			$("div.cate ul").each(function(){
				$(this).removeClass("expanded");
				$(this).addClass("folded");
				$(this).parent().find(".arrow").attr("src", "./img/i_arrow_right.jpg");
		  	});
		  	
 			sConClzId = "about_c_" + sId;
 			$("." + sConClzId).css({"display": "block"});
 		} else {
 			if ($(oUl).hasClass("expanded")) {
 				$(oUl).removeClass("expanded");
				$(oUl).addClass("folded");
				$(oUl).parent().find(".arrow").attr("src", "./img/i_arrow_right.jpg");
 			} else {
 				$(oUl).removeClass("folded");
				$(oUl).addClass("expanded");
				$(oUl).parent().find(".arrow").attr("src", "./img/i_arrow_down.jpg");
 			}
 			
 			sTabsId = $(oUl).attr("id");
 			sConClzId = sTabsId.replace("tabs", "about_c");
 			 			
 			$("div.cate ul").each(function(){
 				
 				if ($(this).attr("id") != sTabsId) {
					$(this).removeClass("expanded");
					$(this).addClass("folded");
					$(this).parent().find(".arrow").attr("src", "./img/i_arrow_right.jpg");
				}
		  	});

 			$("ul." + sTabsId).tabs("div.panes > ." + sConClzId);
 		}
 	});
 
 	$("div.cate .title").first().click();

});
</script>

