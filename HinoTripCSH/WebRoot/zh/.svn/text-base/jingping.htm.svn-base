<script type="text/javascript" src="js/jquery.min.js"></script>
<script type="text/javascript" src="js/jquery.cookies.2.2.0.min.js"></script>
<script defer src="js/jquery.flexslider.js"></script>
<script type="text/javascript">
	$(function() {
		SyntaxHighlighter.all();
	});
	$(window).load(function() {
		$('.flexslider').flexslider({
			animation : "slide",
			start : function(slider) {
				$('body').removeClass('loading');
			}
		});
	});
</script>
</head>
<body>
	<div id="jingping_main" class="jingping_main" align="center">
		<div id="jingping_top" class="jingping_top"></div>
		<div id="jingping_tuan" class="jingping_tuan">
			<!-- Place somewhere in the <body> of your page -->
			<div class="flexslider" style="width:1050px; height:344px; background:none">
				<ul class="slides">
					#foreach($DGroup in $DistGroupList)
					<li>
						<ul>
							<li style="z-index:-1">
								<a href="route_detail.htm?routeid=$DGroup.getRoute().getId()" target="_blank">
									<img alt="$DGroup.getTitle()" src="../$pathpre$DGroup.getImage()" title="$DGroup.getTitle()" height="344px" width="300px"/>
								</a>
							</li>
							
							<li style="float:left;margin-top:-344px;text-align:left; z-index:3000">
								<div style="height:100%; width:450px; padding-left:105px; padding-top:80px; color:white;">
									<div style="font-size:20px; letter-spacing: 1px;">
										￡$DGroup.getPrice()
									</div>
									<div style="font-size:30px; letter-spacing: 1px;">
										$DGroup.getTitle()<br/>
									</div>
									<br/>
									<div style="font-size: 11px; letter-spacing: 1px; width:355px; ">
									&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;$DGroup.getDescription()<br/><br/><br/>
										<div class="route_detail_link">
											<a href="route_detail.htm?routeid=$DGroup.getRoute().getId()">查看路线详情 →</a>
										</div>
									</div>
								</div>
							</li>
						</ul>
					</li> 	
					#end
				</ul>
			</div>
		</div>
		<div id="jingping_service_options" calss="jingping_service_options"></div>
